{
  "lexicon": 1,
  "id": "network.comind.blips.noteToSelf",
  "revision": 1,
  "description": "A note to self node in the comind network",
  "defs": {
    "main": {
      "type": "record",
      "key": "tid",
      "record": {
        "type": "object",
        "required": ["text", "createdAt", "completed"],
        "properties": {
          "text": { "type": "string", "maxLength": 3000 },
          "createdAt": { "type": "string", "format": "datetime" },
          "reminderDate": { "type": "string", "format": "datetime" },
          "completed": { "type": "boolean" }
        }
      }
    }
  }
}
